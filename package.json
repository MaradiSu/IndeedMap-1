{
  "name": "jobsite",
  "version": "1.0.0",
  "scripts": {
    "build": "browserify ./app/js/main.js -o ./app/js-dist/bundle.js -t [ babelify --presets [ es2015 ] ]",
    "watch": "watchify ./app/js/main.js -o ./app/js-dist/bundle.js -t [ babelify --presets [ es2015 ] ] -v"
  },
  "dependencies": {
    "babel-preset-es2015": "^6.24.1",
    "babelify": "^7.3.0",
    "browserify": "^14.4.0",
    "esri-loader": "^1.1.0",
    "watchify": "^3.9.0"
  }
}
